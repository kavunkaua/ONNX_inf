{
    "configurations": [
      {
        "name": "(gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/InferenceONNX_test",
        "args": [""],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/build/",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          },
          {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
          }]
     },
        {
          "name": "Python: Current File",
          "type": "debugpy",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "cwd": "${workspaceFolder}/src/py",
          "justMyCode": true,
          "python": "${workspaceFolder}/src/py/venv/bin/python",
          "args": [],
        }
    ]
}